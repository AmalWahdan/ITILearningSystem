@model List<TraineeCourseDegreeViewModel>


<div class="container-fluid">
    <div class="row text-center justify-content-center">
        <div class="col-lg-6 text-center justify-content-center mb-3">
            <h3>@Model[0].TraineeName </h3>
            <table class="table ">
                <thead class="thead-dark">
                    <tr>
                        <th scope="col">Course Name</th>
                        <th scope="col">Degree</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(TraineeCourseDegreeViewModel TR in Model){
                    <tr style="color:@TR.Color">
                        <td>@TR.CourseName</td>
                        <td>@TR.TraineeDegree</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
